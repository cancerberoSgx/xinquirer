{
  "name": "xinquirer",
  "version": "0.0.1",
  "description": "like Inquirer.js but for X. Ask user questions showing annoying dialogs on top of the current window. based on electron. node.js API.",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc && cp -rf src/assets dist/src",
    "start1": "rm -rf dist/ && npm run build && electron dist/examples/selectFileSimpleExample.js",
    "run1": "node electron dist/examples/selectFileSimpleExample.js",
    "start": "electron",
    "prepare": "npm run build"
  },
  "keywords": [
    "inquirer",
    "electron",
    "data",
    "input",
    "UI",
    "X",
    "filechooser",
    "dialogs",
    "compatible",
    "desktop",
    "node.js"
  ],
  "author": "Sebsti√°n Gurin",
  "license": "ISC",
  "dependencies": {
    "electron": "^2.0.0",
    "p-map-series": "^1.0.0"
  },
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/node": "^10.0.8",
    "@types/p-map-series": "^1.0.1",
    "typescript": "^2.8.3"
  }
}
