{
  "name": "xinquirer",
  "version": "0.0.1",
  "description": "Like Inquirer.js but for with dialog windows. node.js API and CLI to to ask user to enter data, dialogs on top of the current window.",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc && cp -rf src/assets dist/src",
    "start1": "rm -rf dist/ && npm run build && electron dist/examples/selectFileSimpleExample.js",
    "run1": "node electron dist/examples/selectFileSimpleExample.js",
    "start": "electron",
    "prepare": "npm run build"
  },
  "author": "Sebasti√°n Gurin",
  "license": "ISC",
  "dependencies": {
    "electron": "^2.0.0",
    "hard-rejection": "^1.0.0",
    "p-map-series": "^1.0.0"
  },
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/node": "^10.0.8",
    "@types/p-map-series": "^1.0.1",
    "typescript": "^2.8.3"
  },
  "keywords": [
    "inquirer",
    "electron",
    "data",
    "input",
    "UI",
    "X",
    "filechooser",
    "dialogs",
    "compatible",
    "desktop",
    "node.js",
    "dialog"
  ]
}
