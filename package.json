{
  "name": "xinquirer",
  "version": "0.0.1",
  "description": "like inquierer but for X - dialogs that pop up on top of user's window to ask for information",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc && cp -rf src/assets dist/src",
    "start": "rm -rf dist/ && npm run build && electron dist/examples/selectFileSimpleExample.js",
    "run": "node node_modules/electron/cli.js dist/examples/selectFileSimpleExample.js",
    "prepare": "npm run build"
  },
  "keywords": [
    "inquirer",
    "electron",
    "data",
    "input",
    "UI",
    "X",
    "filechooser",
    "dialogs",
    "compatible",
    "desktop",
    "node.js"
  ],
  "author": "Sebsti√°n Gurin",
  "license": "ISC",
  "dependencies": {
    "electron": "^2.0.0"
  },
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/node": "^10.0.8",
    "typescript": "^2.8.3"
  }
}